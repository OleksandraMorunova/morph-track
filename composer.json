{
  "name": "om/morph-track",
  "description": "Track and document changes in Laravel API endpoints, migrations, seeders, and Artisan commands. Automatically generate structured, readable change instructions",
  "keywords": [
    "laravel",
    "api",
    "endpoints",
    "migrations",
    "seeders",
    "artisan",
    "changelog",
    "laravel package",
    "code changes",
    "diff tool",
    "developer tools"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleksandra Morunova",
      "email": "oleksandramorunova@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=9.0",
    "symfony/yaml": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "OM\\MorphTrack\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OM\\MorphTrack\\AnalyzeEndpointServiceProvider"
      ],
      "aliases": {
        "MarkdownTranslator": "OM\\MorphTrack\\JIraTranslator\\Facades\\MarkdownTranslator"
      }
    }
  },
  "version": "1.0.0"
}
